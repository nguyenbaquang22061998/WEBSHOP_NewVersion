@model DOLPHIN.Model.Users

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h3 style="margin-left:20px ; margin-top:20px">Bạn có chắc chắn muốn xóa tài khoản @Model.UserName?</h3>
<div class="card">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <input type="submit" value="Xóa" class="btn btn-danger" style="margin-left:20px" />
        <a href="/Admin/Users/Index" class="btn btn-primary" style="margin-left:20px">Quay lại</a>
    }
</div>
